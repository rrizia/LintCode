public class Solution {
    /*
        1 - 9 return digit
        10 = 1;
        11 = 2;
        12 = 3
        13 = 4
        ......
        18 = 9;
        19 = 10 % 9 = 1;
        20 = 2 % 9 = 2;
        
        so num % 9 == 0?0:mod;
    */
    public int addDigits(int num) {
        if(num<=9) return num;
        int mod = num%9;
        return (mod == 0)? 9: mod;
    }
}
